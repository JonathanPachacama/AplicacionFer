            </form>
            <br><br>
            <div class="row">
              <% for(var i=0; i<resource.length; i++) {%>
                <div class="card" style="width: 20rem;">
                  <div class="card-block">
                    <h4 class="card-title"><%= resource[i].resourceId %> <%= resource[i].resourceTitle %> </h4>
                    <a href="/RecommenderModule/RecommenderWkx/bringParametersCreator?resourceId=<%= resource[i].resourceId %>">
                      <button type="button"  class="btn btn-primary">recomendaci√≥n</button>
                    </a>

                  </div>
