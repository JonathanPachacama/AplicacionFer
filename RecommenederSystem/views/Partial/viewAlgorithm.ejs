                  </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-6"></div>
                <!-- ./col -->
              </div>
              <div class="row" >
                <div class="col-md-3 col-sm-3 col-6">
                  <div class="info-box">
                    <span class="info-box-icon bg-warning"><i class="fa fa-files-o"></i></span>
                    <div class="info-box-content">
                      <span class="info-box-text">resultados</span>
                      <span class="info-box-number">{{size}}</span>
                    </div>
                    <!-- /.info-box-content -->
                  </div>
                  <!-- /.info-box -->
                </div>
                <!-- /.col -->
                <div class="col-md-3 col-sm-3 col-6"></div>
                <!-- /.col -->
                <div class="col-md-3 col-sm-3 col-6"></div>
                <!-- /.col -->
                <div class="col-md-3 col-sm-3 col-6"></div>
                <!-- /.col -->
              </div>
              <!--- view result -->
              <%-partial ('../Partial/result.ejs')%>
              <!-- /.view result -->
            </div>
            <!-- /.tab-pane -->
          </div>
          <!-- /.recommender DBLP -->



        </div>
        <!-- /.tab-content -->
      </div><!-- /.card-body -->
    </div>
    <!-- ./card -->
    <!-- END CUSTOM TABS -->



  </div>
  <!-- /.col -->



</div>
<!-- /.row -->
<!-- /.result WITH ALGORITHM -->




