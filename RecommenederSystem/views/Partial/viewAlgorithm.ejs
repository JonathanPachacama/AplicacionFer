                      <p>acm.rkbexplorer.com</p>
                    </div>
                    <div class="icon">
                      <i class="ion ion-stats-bars"></i>
                    </div>
                    <a href="http://acm.rkbexplorer.com/sparql/" class="small-box-footer" target="_blank">
                      Mas Informacíon <i class="fa fa-arrow-circle-right"></i>
                    </a>
                  </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-6"></div>
                <!-- ./col -->
              </div>
              <div class="row" >
                <div class="col-md-3 col-sm-3 col-6">
                  <div class="info-box">
                    <span class="info-box-icon bg-success"><i class="fa fa-files-o"></i></span>
                    <div class="info-box-content">
                      <span class="info-box-text">resultados</span>
                      <span class="info-box-number">{{size}}</span>
                    </div>
                    <!-- /.info-box-content -->
                  </div>
                  <!-- /.info-box -->
                </div>
                <!-- /.col -->
                <div class="col-md-3 col-sm-3 col-6"></div>
                <!-- /.col -->
                <div class="col-md-3 col-sm-3 col-6"></div>
                <!-- /.col -->
                <div class="col-md-3 col-sm-3 col-6"></div>
                <!-- /.col -->
              </div>
              <!--- view result -->
              <%-partial ('../Partial/result.ejs')%>
              <!-- /.view result -->
            </div>
            <!-- /.tab-pane -->
          </div>
          <!-- /.recommender ACM -->

          <!-- recommender DBLP -->
          <div ng-if="showDBLP">
            <div class="tab-pane" id="dblp">
              <input hidden id="endpointDynamic" class="span5" value="<%= MasterUrlService.base_proxy()%>http://dblp.rkbexplorer.com/sparql/" type="text"  size=30 >
